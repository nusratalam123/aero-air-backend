generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id            String  @id @default(uuid())
  firstName    String?
  lastName     String?
  email        String  @unique
  password     String
  address      String?
  country      String?
  district     String?
  mobileNumber String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}